{
  "manifest_version": 2,
  "name": "News Feed Blocker",
  "version": "0.1",
  "content_scripts": [
  {
    "matches": [
      "https://www.facebook.com/*"
    ],
    "js": ["jquery-2.2.4.min.js","content.js"]
  }
],
  "background": {
  "scripts": ["background.js"]
},
 "browser_action": {},
 "icons": { "16": "NFB16.png",
           "48": "NFB48.png",
          "128": "NFB128.png" }
}
